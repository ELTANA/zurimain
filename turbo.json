{
  "$schema": "https://turbo.build/schema.json",
  "pipeline": {
    "build": {
      "dependsOn": ["^build"],
      "outputs": ["dist/**"]
    },
    "lint": {
      "outputs": [],
      "cache": false
    },
    "dev": {
      "cache": false
    }
  },
  "globalEnv": [
    "BASE_API_URL",
    "BASE_CLIENT_URL",
    "WS_CONNECTION_URL",
    "NODE_ENV",
    "ENV_FILE"
  ]
}
